{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "cross-env NEXT_PUBLIC_TAURI=1 npm run export",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../out"
  },
  "package": {
    "productName": "mooc-helper",
    "version": "0.2.0"
  },
  "tauri": {
    "allowlist": {
      "http": {
        "request": true,
        "scope": [
          "https://www.icourse163.org/*"
        ]
      },
      "shell": {
        "open": true,
        "scope": []
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "tg.cos.mooc",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://mooc.cos.tg/api/update/{{target}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDEzOTYxMDE3M0JCQUNDNEEKUldSS3pMbzdGeENXRTdyN3dFdXpwcC9uVEdSZXdhN2ZxZ0NJVHRmS29CeG05bW0vVFl1U040ejcK"
    },
    "windows": [
      {
        "fullscreen": false,
        "center": true,
        "width": 1024,
        "height": 768,
        "resizable": true,
        "title": "mooc-helper"
      }
    ]
  }
}